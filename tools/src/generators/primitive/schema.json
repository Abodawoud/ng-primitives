{
  "$schema": "https://json-schema.org/schema",
  "$id": "Primitive",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "documentation": {
      "type": "string",
      "description": "What documentation be added for this primitive?",
      "enum": ["None", "Getting Started", "Primitives", "Interactions"]
    }
  },
  "required": ["name", "documentation"]
}
